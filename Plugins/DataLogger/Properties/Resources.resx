  <data name="TemplateCustomShipSorter" xml:space="preserve">
    <value>//以下代码将舰娘名字去掉“改二”和“改”之后，反转，并按反转后的名字排序

//引入必要的命名空间
using System;       //标准系统命名空间
using System.Linq;  //Linq扩展方法空间
using LynLogger;    //插件主命名空间
using LynLogger.DataStore.MasterInfo; //插件主数据表命名空间

//标注处理排序用的类名称 CustomSorter.Sorter
[assembly: ComparerClass("CustomSorter.Sorter")]

//定义命名空间CustomSorter
namespace CustomSorter {

  //定义公开类Sorter 继承于LynLogger.CustomComparerBase&lt;LynLogger.Models.Ship&gt;
  public class Sorter : CustomComparerBase&lt;Ship&gt; {

    //重写父类的比较方法
    public override int Compare(Ship x, Ship y) {

      //返回过滤并反转后的舰名之间的比较结果
      return TrimReverseShipName(x).CompareTo(TrimReverseShipName(y));
    }

    //自定义私有方法TrimReverseShipName，用于过滤并反转舰名
    private string TrimReverseShipName(Ship x) {

      //返回处理后的字符串
      return new string(x.ShipInfo             //获取舰种
                         .ShipName             //获取名称
                         .Replace("改二", "")  //将“改二”删除
                         .Replace("改", "")    //将“改”删除
                         .Reverse()            //反转名称
                         .ToArray());          //转换为数组以用于建立字符串
    }
  }
}</value>
